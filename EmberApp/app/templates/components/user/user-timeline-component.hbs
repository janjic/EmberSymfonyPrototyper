<ul class="timeline">
    <li class="clearfix upgrade">
        <span class="label label-date">25.1.2017</span>
        <div class="timeline-badge primary">
            {{fa-icon "clock-o"}}
        </div>
        <div class="timeline-panel bg-primary">
            <div class="timeline-heading text-white">
                <h5 class="timeline-title">Promoted to <span class="role">Active Agent</span> from <span class="role">Referee</span></h5>
            </div>
            <div class="timeline-body text-white">
                <p>
                   Sales before upgrade: <span>20</span>
                </p>
                <p>
                    Agents before upgrade: <span>2</span>
                </p>
                <p>
                    <small>changed by <span class="img-holder"><img src="../../assets/images/user-avatar.png" alt=""></span><a href="#">Agent Agentovic</a></small>
                </p>
            </div>
            <span class="panel-icon">
                {{fa-icon "angle-double-up"}}
            </span>
        </div>
    </li>
    <li class="timeline-inverted downgrade clearfix">
        <span class="label label-date">25.1.2017</span>
        <div class="timeline-badge danger">
            {{fa-icon "clock-o"}}
        </div>
        <div class="timeline-panel bg-danger">
            <div class="timeline-heading text-white">
                <h5 class="timeline-title">Downgraded to <span class="role">Referee</span> from <span class="role">Active Agent</span></h5>
            </div>
            <div class="timeline-body text-white">
                <p>
                    Sales before downgrade: <span>20</span>
                </p>
                <p>
                    Agents before downgrade: <span>2</span>
                </p>
                <p>
                    <small>changed by <span class="img-holder"><img src="../../assets/images/user-avatar.png" alt=""></span><a href="#">Agent Agentovic</a></small>
                </p>
            </div>
            <span class="panel-icon">
                {{fa-icon "angle-double-down"}}
            </span>
        </div>
    </li>
    <li class="clearfix suspend timeline-inverted">
        <span class="label label-date">25.1.2017</span>
        <div class="timeline-badge info">
            {{fa-icon "clock-o"}}
        </div>
        <div class="timeline-panel bg-info">
            <div class="timeline-heading text-white">
                <h5 class="timeline-title">Promoted to <span class="role">Active Agent</span> from <span class="role"></span></h5>
            </div>
            <div class="timeline-body text-white">
                <p>
                    Sales: <span>20</span>
                </p>
                <p>
                   Agents: <span>2</span>
                </p>
                <p>
                    <small>changed by <span class="img-holder"><img src="../../assets/images/user-avatar.png" alt=""></span><a href="#">Agent Agentovic</a></small>
                </p>
            </div>
            <span class="panel-icon panel-icon-ban">
                {{fa-icon "ban"}}
            </span>
        </div>
    </li>
</ul>
