<section class="form-wizard">
    <h1 class="main-heading">Agent edit</h1>
    <form role="form">
        <div class="col-md-2 col-sm-6 col-xs-12">
            <img class="img img-responsive" src="../../assets/images/user-avatar.png" alt="">
            <hr>
            <div class="form-group clearfix">
                <a href="change-password.html" class="btn btn-danger">Change password</a>
            </div>
        </div>
        <div class="col-md-5 col-sm-6 col-xs-12">
            <h3 class="sub-heading">Account information</h3>
            <div class="profile-settings">
                <div class="row">
                    <div class="form-group clearfix">
                        <label class="control-label col-md-4 col-xs-4">Title</label>
                        <div class="col-md-8 col-xs-8">
                            <select name="type" id="typeselect" class="form-control">
                                <option value="1" selected>Mr.</option>
                                <option value="2">Mrs.</option>
                                <option value="2">Ms.</option>
                            </select>
                        </div>
                    </div>
                    <div class="form-group clearfix">
                        <label class="control-label col-md-4 col-xs-4">First name</label>
                        <div class="col-md-8 col-xs-8">
                            <input class="form-control" type="text" placeholder="John">
                        </div>
                    </div>
                    <div class="form-group clearfix">
                        <label class="control-label col-md-4 col-xs-4">Last name</label>
                        <div class="col-md-8 col-xs-8">
                            <input class="form-control" type="text" placeholder="Johnson">
                        </div>
                    </div>
                    <div class="form-group clearfix">
                        <label class="control-label col-md-4 col-xs-4">Email</label>
                        <div class="col-md-8 col-xs-8">
                            <input disabled class="form-control" type="text" placeholder="nkm123@gmail.com">
                        </div>
                    </div>
                    <div class="form-group clearfix">
                        <label class="control-label col-md-4 col-xs-4">Phone number</label>
                        <div class="col-md-8 col-xs-8">
                            <input class="form-control" type="text" placeholder="+42 42 234 56 67">
                        </div>
                    </div>
                    <div class="form-group clearfix">
                        <label class="control-label col-md-4 col-xs-4">Language</label>
                        <div class="col-md-8 col-xs-8">
                            <input class="form-control" type="text" placeholder="English">
                        </div>
                    </div>
                    <div class="form-group clearfix">
                        <label class="control-label col-md-4 col-xs-4">Country</label>
                        <div class="col-md-8 col-xs-8">
                            <input class="form-control" type="text" placeholder="Switzerland">
                        </div>
                    </div>
                    <div class="form-group clearfix">
                        <label class="control-label col-md-4 col-xs-4">Social security number</label>
                        <div class="col-md-8 col-xs-8">
                            <input class="form-control" type="text" placeholder="234WX123WQ">
                        </div>
                    </div>
                    <div class="form-group clearfix">
                        <label class="control-label col-md-4 col-xs-4">Bank name</label>
                        <div class="col-md-8 col-xs-8">
                            <input class="form-control" type="text" placeholder="John">
                        </div>
                    </div>
                    <div class="form-group clearfix">
                        <label class="control-label col-md-4 col-xs-4">Bank account number or IBAN</label>
                        <div class="col-md-8 col-xs-8">
                            <input class="form-control" type="text" placeholder="Smith">
                        </div>
                    </div>
                    <div class="form-group clearfix">
                        <label class="control-label col-md-4 col-xs-4">Date of birth</label>
                        <div class="col-md-8 col-xs-8">
                            <input class="form-control" type="text" placeholder="04/02/1991">
                        </div>
                    </div>
                    <div class="form-group clearfix">
                        <label class="control-label col-md-4 col-xs-4">Status</label>
                        <div class="col-md-8 col-xs-8">
                            <div class="onoffswitch">
                                <input type="checkbox" name="onoffswitch" class="onoffswitch-checkbox" id="myonoffswitch" checked>
                                <label class="onoffswitch-label" for="myonoffswitch">
                                    <span class="onoffswitch-inner active-switch"></span>
                                    <span class="onoffswitch-switch"></span>
                                </label>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-md-5 col-sm-6 col-xs-12">
            <h3 class="sub-heading">Optional information</h3>
            <div class="profile-settings">
                <div class="row">
                    <div class="form-group clearfix">
                        <label class="control-label col-md-4 col-xs-4">Address</label>
                        <div class="col-md-8 col-xs-8">
                            <input class="form-control" type="text" placeholder="Miloja Djaka 36, Belgrade">
                        </div>
                    </div>
                    <div class="form-group clearfix">
                        <label class="control-label col-md-4 col-xs-4">ZIP Code</label>
                        <div class="col-md-8 col-xs-8">
                            <input class="form-control" type="text" placeholder="11000">
                        </div>
                    </div>
                    <div class="form-group clearfix">
                        <label class="control-label col-md-4 col-xs-4">Private email</label>
                        <div class="col-md-8 col-xs-8">
                            <input class="form-control" type="text" placeholder="nkm790021@gmail.com">
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-md-5 col-sm-6 col-xs-12">
            <h3 class="sub-heading">Genealogy tree</h3>
            <div class="profile-settings">
                <div class="row">
                    <div class="form-group clearfix">
                        <label class="control-label col-md-4 col-xs-4">Agent ID</label>
                        <div class="col-md-8 col-xs-8">
                            <input class="form-control" type="text" placeholder="32432fdsf">
                        </div>
                    </div>
                    <div class="form-group clearfix">
                        <label class="control-label col-md-4 col-xs-4">Agent type</label>
                        <div class="col-md-8 col-xs-8">
                            <input class="form-control" type="text" placeholder="Master agent">
                        </div>
                    </div>
                    <div class="form-group clearfix">
                        <label class="control-label col-md-4 col-xs-4">Direct superior</label>
                        <div class="col-md-8 col-xs-8">
                            <input class="form-control" type="text" placeholder="Tom Johnson">
                        </div>
                    </div>
                    <div class="form-group clearfix">
                        <label class="control-label col-md-4 col-xs-4">Number of agents downline</label>
                        <div class="col-md-8 col-xs-8">
                            <input class="form-control" type="text" placeholder="7 agents">
                        </div>
                    </div>
                    <div class="form-group clearfix">
                        <label class="control-label col-md-4 col-xs-4">Number of customers</label>
                        <div class="col-md-8 col-xs-8">
                            <input class="form-control" type="text" placeholder="15 customers">
                        </div>
                    </div>
                    <div class="form-group clearfix">
                        <label class="control-label col-md-4 col-xs-4">Total sum of customers orders</label>
                        <div class="col-md-8 col-xs-8">
                            <input class="form-control" type="text" placeholder="43534 CHF">
                        </div>
                    </div>
                    <div class="form-group clearfix">
                        <div class="col-md-12">
                            <a href="agent-edit.html" class="btn btn-success pull-right">Save agent profile</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </form>

</section>